public class MainAnimal {
	public static void main(String[]args)
    {
    	
   Cat a1 = new Cat("Meow. I am a cat. My name is Kurre.");
   a1.print1();
   
   System.out.println();	
   System.out.println();
   
   Dog a2 = new Dog("Woof. I am a cat. My name is Vilma.");
   a2.print();
   
   System.out.println();	
   System.out.println();
   
   Cat a3 = new Cat("Meow. I am a cat. My name is Bamse.");
   a3.print2();
   
   System.out.println();	
   System.out.println();
   
  			
    }
}
